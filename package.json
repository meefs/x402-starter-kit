{
  "name": "x402-starter-kit",
  "version": "0.0.1",
  "description": "Starter kit for building paid APIs using the x402 payment protocol",
  "main": "dist/server.js",
  "type": "module",
  "bin": {
    "create-x402": "./cli.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "start:facilitator": "node dist/facilitator.js",
    "dev": "tsc && node dist/server.js",
    "dev:facilitator": "tsc && node dist/facilitator.js",
    "test": "npm run build && node dist/testClient.js",
    "test:solana": "npm run build && node dist/testClientSolana.js",
    "test:shell": "bash test-request.sh",
    "setup:solana": "npm run build && node dist/setupSolanaWallets.js",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "x402",
    "payment-api",
    "payments",
    "usdc",
    "eip-3009",
    "solana",
    "svm"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@scure/base": "^2.0.0",
    "@solana/spl-token": "^0.4.9",
    "@solana/web3.js": "^1.98.0",
    "@x402/core": "^2.0.0",
    "@x402/evm": "^2.0.0",
    "@x402/svm": "^2.0.0",
    "dotenv": "^16.4.5",
    "ethers": "^6.15.0",
    "express": "^4.21.2",
    "openai": "^4.78.0",
    "viem": "^2.43.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^24.6.2",
    "typescript": "^5.9.3"
  }
}
